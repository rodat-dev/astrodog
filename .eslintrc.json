{
  "extends": ["next","next/core-web-vitals", "next/typescript","turbo","prettier"],
  "plugins": ["react-compiler","turbo"],
  "rules": {
    "react-compiler/react-compiler": "error",
    "turbo/no-undeclared-env-vars": [
      "error",
      {
        "allowList": ["^ENV_[A-Z]+$"]
      }
    ]
  }
}
